PREFIX dtl:  <https://linkedmusic.ca/graphs/dig-that-lick/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX wdt:  <http://www.wikidata.org/prop/direct/>
PREFIX wd:   <http://www.wikidata.org/entity/>

SELECT DISTINCT
  ?track ?trackLabel
  ?recordingLocation
WHERE {
  GRAPH <https://linkedmusic.ca/graphs/dig-that-lick/> {
    ?track rdf:type dtl:Track ;
           wdt:P8546 wd:Q60 .

    OPTIONAL { ?track rdfs:label ?trackLabel . }

    # return the location URI as requested (it will be wd:Q60 here)
    BIND(wd:Q60 AS ?recordingLocation)
  }
}
ORDER BY LCASE(STR(?trackLabel)) STR(?track)