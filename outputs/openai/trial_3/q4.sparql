PREFIX gj:   <https://linkedmusic.ca/graphs/theglobaljukebox/>
PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX wdt:  <http://www.wikidata.org/prop/direct/>

SELECT DISTINCT ?song ?songLabel
WHERE {
  GRAPH <https://linkedmusic.ca/graphs/theglobaljukebox/> {
    # Verify type (local LinkedMusic type)
    ?song rdf:type gj:Song .

    OPTIONAL { ?song rdfs:label ?songLabel . }

    # Ethiopian songs: country of origin = Ethiopia (Wikidata Q115)
    ?song wdt:P495 <http://www.wikidata.org/entity/Q115> .
  }
}
ORDER BY LCASE(STR(?songLabel)) STR(?song)