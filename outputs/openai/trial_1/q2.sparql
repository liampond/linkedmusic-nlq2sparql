PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ts:   <https://linkedmusic.ca/graphs/thesession/>
PREFIX wdt:  <http://www.wikidata.org/prop/direct/>

SELECT DISTINCT
  ?session ?sessionLabel
WHERE {
  GRAPH <https://linkedmusic.ca/graphs/thesession/> {
    # Verify type (LinkedMusic type, not Wikidata)
    ?session rdf:type ts:Session .

    # Country is stored directly as a Wikidata URI in The Session data
    ?session wdt:P17 <http://www.wikidata.org/entity/Q142> .

    OPTIONAL { ?session rdfs:label ?sessionLabel . }
  }
}
ORDER BY LCASE(STR(?sessionLabel)) STR(?session)