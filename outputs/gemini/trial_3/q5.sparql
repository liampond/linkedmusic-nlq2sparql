PREFIX dtl: <https://linkedmusic.ca/graphs/dig-that-lick/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?solo ?label
WHERE {
  GRAPH <https://linkedmusic.ca/graphs/dig-that-lick/> {
    # Identify entities explicitly typed as Solos in the DTL graph
    ?solo rdf:type dtl:Solo .
    
    # Retrieve the label if available
    OPTIONAL { ?solo rdfs:label ?label }
  }
}