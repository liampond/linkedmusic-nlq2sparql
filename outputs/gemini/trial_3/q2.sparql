PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ts: <https://linkedmusic.ca/graphs/thesession/>

SELECT DISTINCT ?session ?label
WHERE {
  GRAPH <https://linkedmusic.ca/graphs/thesession/> {
    # Verify the entity is a Session using the local type
    ?session rdf:type ts:Session .
    
    # Match the country property directly to the Wikidata Q-ID for France (Q142)
    ?session wdt:P17 wd:Q142 .
    
    # Retrieve the label if available
    OPTIONAL { ?session rdfs:label ?label . }
  }
}