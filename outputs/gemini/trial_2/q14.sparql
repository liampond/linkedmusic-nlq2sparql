PREFIX gj: <https://linkedmusic.ca/graphs/theglobaljukebox/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX wd: <http://www.wikidata.org/entity/>

SELECT ?song ?label
WHERE {
  # Subquery to retrieve Songs and their Countries from the local LinkedMusic graph
  {
    SELECT ?song ?label ?country
    WHERE {
      GRAPH <https://linkedmusic.ca/graphs/theglobaljukebox/> {
        ?song rdf:type gj:Song .
        ?song rdfs:label ?label .
        ?song wdt:P495 ?country .
      }
    }
  }

  # Federated query to Wikidata to filter countries located in Africa (Q15)
  SERVICE <https://query.wikidata.org/sparql> {
    ?country wdt:P30 wd:Q15 .
  }
}