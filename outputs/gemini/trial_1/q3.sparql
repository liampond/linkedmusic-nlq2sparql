PREFIX mb: <https://linkedmusic.ca/graphs/musicbrainz/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?recording ?recordingLabel
WHERE {
    GRAPH <https://linkedmusic.ca/graphs/musicbrainz/> {
        # Identify the Artist entity for Taylor Swift using her Wikidata Q-ID (Q26876)
        ?artist wdt:P2888 <http://www.wikidata.org/entity/Q26876> ;
                rdf:type mb:Artist .

        # Find recordings where Taylor Swift is the performer (P175)
        ?recording wdt:P175 ?artist ;
                   rdf:type mb:Recording ;
                   rdfs:label ?recordingLabel .
    }
}